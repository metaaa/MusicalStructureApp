    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.musicalstructureapp.favsActivity"
    android:background="#051721">

    <include layout="@layout/toolbar_layout"/>

    <TextView
        android:id="@+id/fav1"
        style="@style/favlist"
        android:text="@string/favs_description"/>

    <TextView
        android:id="@+id/fav2"
        style="@style/favlist"/>

    <TextView
        android:id="@+id/fav3"
        style="@style/favlist"/>

    <TextView
        android:id="@+id/fav4"
        style="@style/favlist"/>

</LinearLayout>
